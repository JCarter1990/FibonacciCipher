<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="200.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cipherui.FXMLDocumentController">
    <children>
        <Button fx:id="button" layoutX="10.0" layoutY="90.0" onAction="#handleEncryption" prefHeight="25.0" prefWidth="60.0" text="Encrypt" />
        <Button fx:id="button1" layoutX="230.0" layoutY="90.0" onAction="#handleDecryption" prefHeight="25.0" prefWidth="60.0" text="Decrypt" />
        <Label layoutX="160.0" layoutY="60.0" prefHeight="25.0" prefWidth="25.0" text="Key: " />
        <Label layoutX="20.0" layoutY="120.0" text="Output" />
        <Label layoutX="20.0" layoutY="5.0" text="Input" />
        <TextArea fx:id="displayString" layoutX="10.0" layoutY="140.0" prefHeight="50.0" prefWidth="280.0" />
        <TextField fx:id="inputString" layoutX="10.0" layoutY="25.0" prefHeight="25.0" prefWidth="280.0" />
        <TextField fx:id="cipherKey" layoutX="190.0" layoutY="60.0" prefHeight="25.0" prefWidth="100.0" promptText="Enter number" />
    </children>
</AnchorPane>
